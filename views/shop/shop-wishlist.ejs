                                <% user.wishlist.forEach(item => { %>
                                  <tr>
                                    <td class="image product-thumbnail">
                                        <img src="/uploads/croppedProducts/<%= item.productId.images[0]?.url %>" alt="<%= item.productId.title %>">
                                    </td>
                                    <td class="product-des product-name">
                                      <h5 class="product-name"><%= item.productId.title %></h5>
                                      <!-- <p class="font-xs"><%= item.productId.description %></p> -->
                                    </td>
                                    <td class="price" data-title="Price">
                                      <span>$<%= item.productId.regularPrice %></span>
                                    </td>
                                    <td class="text-center" data-title="Stock">
                                      <% if (item.productId.stock > 0) { %>
                                        <span class="color3 font-weight-bold">In Stock</span>
                                      <% } else { %>
                                        <span class="text-danger font-weight-bold">Out of Stock</span>
                                      <% } %>
                                    </td>
                                    <td class="text-right" data-title="Cart">
                                      <% if (item.productId.stock > 0) { %>

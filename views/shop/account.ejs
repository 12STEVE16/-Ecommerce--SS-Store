                                                        <div class="form-group col-md-12">
                                                            <label>Email Address <span class="required"></span></label>
                                                            <input required="" class="form-control square" name="email" type="email" value="<%= user.username %>" readonly>
                                                        </div>
                                                        <div class="form-group col-md-6">
                                                            <label>First Name <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="firstName" type="text" value="<%= user.firstName %>">
                                                        </div>
                                                        <div class="form-group col-md-6">
                                                            <label>Last Name <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="lastName" type="text" value="<%= user.lastName %>">
                                                        </div>
                                                        <div class="form-group col-md-6">
                                                            <label>Phone Number <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="phone" type="text" value="<%= user.mobile %>">
                                                        </div>
                                                        <div class="col-md-12">
                                                            <button type="submit" class="btn btn-fill-out submit" name="submit" value="Submit">Update</button>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                    <% if (user.isLocalAccount) { %>
                                        <div class="tab-pane fade" id="account-password" role="tabpanel" aria-labelledby="account-password-tab">
                                            <div class="card">
                                                <div class="card-header">
                                                    <h5>Password</h5>
                                                </div>
                                                <div class="card-body">
                                                    <form method="post" id="accountPasswordForm">
                                                        <div class="row">
                                                            <div class="form-group col-md-12">
                                                                <label>Current Password <span class="required">*</span></label>
                                                                <input class="form-control square" name="currentPassword" type="password" id="currentPassword">
                                                                <div id="currentPasswordError" style="color: red;"></div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <label>New Password <span class="required">*</span></label>
                                                                <input class="form-control square" name="password" type="password" id="password">
                                                                <div id="passwordError" style="color: red;"></div>
                                                            </div>
                                                            <div class="form-group col-md-12">
                                                                <label>Confirm Password <span class="required">*</span></label>
                                                                <input class="form-control square" name="confirmPassword" type="password" id="confirmPassword">
                                                                <div id="confirmPasswordError" style="color: red;"></div>
                                                            </div>
                                                            <input type="hidden" name="userId" value="<%= user._id %>">
                                                        </div>
                                                        <div class="col-md-12">
                                                            <button type="submit" class="btn btn-fill-out submit" name="submit" id="submit" value="Submit">Update</button>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                        <% } else { %>
                                        <div class="tab-pane fade" id="account-password" role="tabpanel" aria-labelledby="account-password-tab">
                                            <div class="card">
                                                <div class="card-header">
                                                    <h5>Password</h5>
                                                </div>
                                                <div class="card-body">
                                                    <p>Your account was registered using 
                                                        <% if (user.googleId) { %>
                                                            Google
                                                        <% } else if (user.facebookId) { %>
                                                            Facebook
                                                        <% } %>
                                                        . You cannot change the password for this account.
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                        <% } %>
                                        
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

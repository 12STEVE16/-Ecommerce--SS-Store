    <script>
        $(document).ready(function() {
          // Prevent form submission
          $('#search-form').on('submit', function(e) {
            e.preventDefault();
          });
      
          // Show suggestions as user types
          $('#search-input').on('keyup', function() {
            const query = $(this).val();
            if (query.length > 2) {
              $.ajax({
                url: '/search',
                method: 'GET',
                data: { q: query },
                success: function(data) {
                  $('#search-suggestions').empty();
                  data.forEach(product => {
                    $('#search-suggestions').append(`
                    <a href="#"><div class="suggestion-item" data-id="${product._id}">
                        ${product.title}
                      </div>
                    </a>
                    `);
                  });
                }
              });
            } else {
              $('#search-suggestions').empty();
            }
          });
      
          // Redirect to product detail view on suggestion click
          $(document).on('click', '.suggestion-item', function() {
            const productId = $(this).data('id');
            window.location.href = `/products/${productId}`;
          });
        });
      </script>
      <script>
        document.getElementById('search-input').addEventListener('keypress', function (event) {
        if (event.key === 'Enter') {
            event.preventDefault(); // Prevent default Enter behavior
            document.getElementById('search-form').submit(); // Submit the form
        }
        });

    </script>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // Select all the "add-to-wishlist" links
            const wishlistLinks = document.querySelectorAll(".add-to-wishlist");

            wishlistLinks.forEach(link => {
                link.addEventListener("click", async (e) => {
                    e.preventDefault();
                    const productId = link.getAttribute("data-product-id");

                    try {
                        const response = await fetch(`/wishlist/add/${productId}`, {
                            method: "POST",
                            headers: {
                                "Content-Type": "application/json",
                            },
                        });

                        if (response.ok) {
                            window.location.href = '/wishlist'; // Redirect to wishlist on success
                        } else {
                            window.location.href = '/wishlist'; // Redirect to wishlist on failure
                        }
                    } catch (error) {
                        console.error("Error adding to wishlist:", error);
                        window.location.href = '/wishlist'; // Fallback redirect in case of error
                    }
                });
            });
        });

    </script>
    
     

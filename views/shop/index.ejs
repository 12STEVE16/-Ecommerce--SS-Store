                        </div>
                    </div>
                </div>
            </div>
            <div class="slider-arrow hero-slider-1-arrow"></div>
        </section>
        <section class="product-tabs section-padding position-relative wow fadeIn animated">
            <div class="bg-square"></div>
            <div class="container">
                <div class="tab-header">
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                        <!-- <li class="nav-item" role="presentation">
                            <button class="nav-link active" id="nav-tab-one" data-bs-toggle="tab" data-bs-target="#tab-one" type="button" role="tab" aria-controls="tab-one" aria-selected="true">Featured</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link" id="nav-tab-two" data-bs-toggle="tab" data-bs-target="#tab-two" type="button" role="tab" aria-controls="tab-two" aria-selected="false">Popular</button>
                        </li>
                         <li class="nav-item" role="presentation">
                            <button class="nav-link" id="nav-tab-three" data-bs-toggle="tab" data-bs-target="#tab-three" type="button" role="tab" aria-controls="tab-three" aria-selected="false">New added</button>
                        </li> --> 
                    </ul>
                    <a href="/allProducts" class="view-more d-none d-md-flex">View More<i class="fi-rs-angle-double-small-right"></i></a>
                </div>
                <!--End nav-tabs-->
                <div class="tab-content wow fadeIn animated" id="myTabContent">
                    <div class="tab-pane fade show active" id="tab-one" role="tabpanel" aria-labelledby="tab-one">
                        <div class="row product-grid-4">
                             <% products.forEach(product => { %>
                                <div class="col-lg-3 col-md-4 col-12 col-sm-6">
                                    <div class="product-cart-wrap mb-30">
                                        <div class="product-img-action-wrap">
                                            <div class="product-img product-img-zoom">
                                                <a href="/products/<%= product._id %>">
                                                    <img class="default-img" src="/uploads/croppedProducts/<%= product.images[0].url %>" alt="<%= product.title %>">
                                                    <img class="hover-img" src="/uploads/croppedProducts/<%= product.images[1].url %>" alt="<%= product.title %>" alt="">
                                                </a>
                                            </div>
                                            <div class="product-action-1">
                                                <a aria-label="Quick view" class="action-btn hover-up" href="/products/<%= product._id %>"><i class="fi-rs-eye"></i></a>
                                                <!-- <a aria-label="Add To Wishlist" class="action-btn hover-up" href="/wishlist/add/<%= product._id %>"><i class="fi-rs-heart"></i></a> -->
                                                <a href="#" class="action-btn hover-up add-to-wishlist" data-product-id="<%= product._id %>" aria-label="Add To Wishlist"><i class="fi-rs-heart"></i></a>

                                            </div>
                                            <div class="product-badges product-badges-position product-badges-mrg">
                                                <% if (product.list) { %>
                                                    <% const badges = ['Hot', 'New', `${-product.productDiscount}% Off`]; %>
                                                    <% const classForSpan=['hot','new'] %>
                                                    <span class="<%= classForSpan[Math.floor(Math.random() * classForSpan.length)] %>">
                                                        <%= badges[Math.floor(Math.random() * badges.length)] %>
                                                    </span>
                                                <% } %>
                                            </div>
                                        </div>
                                        <div class="product-content-wrap">
                                            <div class="product-category">
                                                <a href="/allProducts?ParentCategory=<%= product.category.parent%>"><%= product.category.parent%></a>
                                            </div>
                                            <h2><a href="/products/<%= product._id %>"><%= product.title %></a></h2>
                                            <div class="rating-result" title="<%= product.ratingPercentage %>">
                                                <span>
                                                    <span><%= product.ratingPercentage %></span>
                                                </span>
                                            </div>
                                            <div class="product-price">
                                                <% if (product.offerPrice) { %>
                                                <span>$<%= product.offerPrice %> </span>
                                                <% } %>
                                                <span class="old-price">$<%= product.regularPrice %></span>
                                            
                                            </div>
                                            <div class="product-action-1 show">
                                                <a aria-label="Add To Cart" class="action-btn hover-up" href="/cart/add/<%= product._id %>"><i class="fi-rs-shopping-bag-add"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                        <% }); %>
                            
                           
                    </div>
                        <!--End product-grid-4-->
                    
                </div>
                <!--End tab-content-->
            </div>
        </section>
        
        
        <section class="section-padding">
            <div class="container wow fadeIn animated">
                <h3 class="section-title mb-20"><span>New</span> Arrivals</h3>
                <div class="carausel-6-columns-cover position-relative">
                    <div class="slider-arrow slider-arrow-2 carausel-6-columns-arrow" id="carausel-6-columns-2-arrows"></div>
                    <div class="carausel-6-columns carausel-arrow-center" id="carausel-6-columns-2">
                        
                        <% newProducts.forEach(newProduct => { %>
                            <div class="product-cart-wrap small hover-up">
                                <div class="product-img-action-wrap">
                                    <div class="product-img product-img-zoom">
                                        <a href="/products/<%= newProduct._id %>">
                                            <img class="default-img" src="/uploads/croppedProducts/<%= newProduct.images[0].url %>" alt="<%= newProduct.title %>">
                                            <img class="hover-img" src="/uploads/croppedProducts/<%= newProduct.images[1].url %>" alt="<%= newProduct.title %>" alt="">
                                        </a>
                                    </div>
                                    <div class="product-action-1">
                                        <a aria-label="Quick view" class="action-btn small hover-up" href="/products/<%= newProduct._id %>">
                                            <i class="fi-rs-eye"></i>
                                        </a>
                                        <!-- <a aria-label="Add To Wishlist" class="action-btn small hover-up" href="/wishlist" tabindex="0">
                                            <i class="fi-rs-heart"></i>
                                        </a> -->
                                        <a href="#" class="action-btn small hover-up add-to-wishlist" data-product-id="<%= newProduct._id %>" aria-label="Add To Wishlist" tabindex="0"><i class="fi-rs-heart"></i></a>

                                    </div>
                                    <div class="product-badges product-badges-position product-badges-mrg">
                                        <% const badges = ['Hot', 'New', `${-newProduct.productDiscount}% Off`]; %>
                                        <% const classForSpan=['hot','new'] %>
                                        <span class="<%= classForSpan[Math.floor(Math.random() * classForSpan.length)] %>">
                                            <%= badges[Math.floor(Math.random() * badges.length)] %>
                                        </span>
                                    </div>
                                </div>
                                <div class="product-content-wrap">
                                    <h2><a href="<%= newProduct.url %>"><%= newProduct.title %></a></h2>
                                    <div class="rating-result" title="<%= newProduct.ratingPercentage %>">
                                        <span>
                                            <span><%= newProduct.ratingPercentage %></span>
                                        </span>
                                    </div>
                                    <div class="product-price">
                                        <span>$<%= newProduct.offerPrice || newProduct.regularPrice %> </span>
                                        <% if (newProduct.offerPrice) { %>
                                            <span class="old-price">$<%= newProduct.regularPrice %></span>
                                        <% } %>
                                    </div>
                                </div>
                            </div>
                        <% }); %>
                       
                        
                        
                    </div>
                </div>
            </div>
        </section>
        <section class="bg-grey-9 section-padding">
            <div class="container pt-25 pb-25">
                <div class="heading-tab d-flex">
                    <div class="heading-tab-left wow fadeIn animated">
                        <h3 class="section-title mb-20"><span>Monthly</span> Best Sell</h3>
                    </div>
                    <div class="heading-tab-right wow fadeIn animated">
                        <ul class="nav nav-tabs right no-border" id="myTab-1" role="tablist">
                            <li class="nav-item" role="presentation">

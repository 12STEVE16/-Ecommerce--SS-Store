                            </div>
                            <div class="sort-by-product-area">
                                
                                <div class="sort-by-cover">
                                    <div class="sort-by-product-wrap">
                                        <div class="sort-by">
                                            <span><i class="fi-rs-apps-sort"></i>Sort by:</span>
                                        </div>
                                        <div class="sort-by-dropdown-wrap">
                                            <span> 
                                                <% if (!sortOrder || sortOrder === 'featured') { %> 
                                                  Featured 
                                                <% } else if (sortOrder === 'priceAsc') { %> 
                                                  Price: Low to High 
                                                <% } else if (sortOrder === 'priceDesc') { %> 
                                                  Price: High to Low 
                                                <% } %>
                                                <i class="fi-rs-angle-small-down"></i>
                                              </span>
                                        </div>
                                    </div>
                                    <div class="sort-by-dropdown">
                                        <ul>
                                          <!-- Sort by Featured -->
                                          <li>
                                            <a class="<%= !sortOrder || sortOrder === 'featured' ? 'active' : '' %>" 
                                               href="/allProducts?sortOrder=featured<%= categoryFilterValue ? (parentCategory ? `&ParentCategory=${categoryFilterValue}` : `&Category=${categoryFilterValue}`) : '' %><%= searchQuery ? `&search=${searchQuery}` : '' %>">
                                              Featured
                                            </a>
                                          </li>
                                      
                                          <!-- Sort by Price: Low to High -->
                                          <li>
                                            <a class="<%= sortOrder === 'priceAsc' ? 'active' : '' %>" 
                                               href="/allProducts?sortOrder=priceAsc<%= categoryFilterValue ? (parentCategory ? `&ParentCategory=${categoryFilterValue}` : `&Category=${categoryFilterValue}`) : '' %><%= searchQuery ? `&search=${searchQuery}` : '' %>">
                                              Price: Low to High
                                            </a>
                                          </li>
                                      
                                          <!-- Sort by Price: High to Low -->
                                          <li>
                                            <a class="<%= sortOrder === 'priceDesc' ? 'active' : '' %>" 
                                               href="/allProducts?sortOrder=priceDesc<%= categoryFilterValue ? (parentCategory ? `&ParentCategory=${categoryFilterValue}` : `&Category=${categoryFilterValue}`) : '' %><%= searchQuery ? `&search=${searchQuery}` : '' %>">
                                              Price: High to Low
                                            </a>
                                          </li>
                                        </ul>
                                    </div>
                                 
                                </div>
                            </div>
                        </div>
                        <div class="row product-grid-3">
                            <% products.forEach(product => { %>
                                <div class="col-lg-3 col-md-4 col-12 col-sm-6">
                                    <div class="product-cart-wrap mb-30">
                                        <div class="product-img-action-wrap">
                                            <div class="product-img product-img-zoom">
                                                <a href="/products/<%= product._id %>">
                                                    <img class="default-img" src="/uploads/croppedProducts/<%= product.images[0].url %>" alt="<%= product.title %>">
                                                    <img class="hover-img" src="/uploads/croppedProducts/<%= product.images[1].url %>" alt="<%= product.title %>" alt="">
                                                </a>
                                            </div>
                                            <div class="product-action-1">
                                                <a aria-label="Quick view" class="action-btn hover-up" data-bs-toggle="modal" data-bs-target="#quickViewModal" href="/products/<%= product._id %>"><i class="fi-rs-eye"></i></a>
                                                <a aria-label="Add To Wishlist" class="action-btn hover-up" href="shop-wishlist.html"><i class="fi-rs-heart"></i></a>
                                            
                                            </div>
                                            <div class="product-badges product-badges-position product-badges-mrg">
                                                <% if (product.list) { %>
                                                    <% const badges = ['Hot', 'New', `${-product.productDiscount}% Off`]; %>
                                                    <% const classForSpan=['hot','new'] %>
                                                    <span class="<%= classForSpan[Math.floor(Math.random() * classForSpan.length)] %>">
                                                        <%= badges[Math.floor(Math.random() * badges.length)] %>
                                                    </span>
                                                <% } %>
                                            </div>
                                        </div>
                                        <div class="product-content-wrap">
                                            <div class="product-category">
                                                <a href=""><%= product.category.parent%></a>
                                            </div>
                                            <h2><a href="<%= product.url %>"><%= product.title %></a></h2>
                                            <div class="rating-result" title="<%= product.ratingPercentage %>">
                                                <span>
                                                    <span><%= product.ratingPercentage %></span>
                                                </span>
                                            </div>
                                            <div class="product-price">
                                                <% if (product.offerPrice) { %>
                                                <span>$<%= product.offerPrice %> </span>
                                                <% } %>
                                                <span class="old-price">$<%= product.regularPrice %></span>
                                            
                                            </div>
                                            <div class="product-action-1 show">
                                                <a aria-label="Add To Cart" class="action-btn hover-up" href="/cart/add/<%= product._id %>"><i class="fi-rs-shopping-bag-add"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% }); %>
                            
                            
                        </div>
